.PHONY = fmt all clean deps

################################################################################

all: sslcli

sslcli:
	go build sslcli.go

deps:
	git config --global http.https://pkg.re.followRedirects true
	go get -d -v pkg.re/essentialkaos/ek.v9
	go get -d -v pkg.re/essentialkaos/sslscan.v7

fmt:
	find . -name "*.go" -exec gofmt -s -w {} \;

clean:
	rm -f sslcli

